CREATE OR REPLACE TRIGGER tmp_trigger
    AFTER UPDATE
    OF first_name
    ON customer
    FOR EACH ROW

DECLARE
    v_user VARCHAR2(64) ;
    
BEGIN
    SELECT user
    INTO v_user
    FROM DUAL ;
    
    INSERT INTO audit_table_log(log_id, userid, event_date, o_value, n_value, commentary)
        VALUES (log_seq.NEXTVAL, v_user, SYSDATE, :OLD.first_name, :NEW.first_name, 'Testing') ;  -- OLD/NEW Pseudo Records
END ;
